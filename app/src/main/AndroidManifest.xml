<?xml version="1.0" encoding="utf-8"?>
<manifest
	package="com.boa.dftest"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<uses-permission android:name="android.permission.INTERNET" />
	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:logo="@mipmap/ic_launcher"
		android:roundIcon="@mipmap/ic_launcher"
		android:supportsRtl="false"
		android:theme="@style/AppTheme">
		<activity android:name="com.boa.dftest.activities.GridActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name="com.boa.dftest.activities.ListActivity" />
		<activity android:name="com.boa.dftest.activities.DetailActivity" />
		<activity
			android:name="com.boa.dftest.activities.PhotoActivity"
			android:configChanges="orientation|keyboardHidden|screenSize"
			android:label="@string/title_activity_photo"
			android:theme="@style/FullscreenTheme" />
	</application>
</manifest>